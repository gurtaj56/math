class Main {
  public static void main(String[] args) {
double radius = 6.2;
 final double PI = 3.14;
    double area;
  area = (PI*radius*radius);
    System.out.println( "Area of circle is"+area);

    double radius1= 5.9;
      double area1 = (PI*radius1*radius1);
    System.out.println("Area of circle is  "+ area1);
  }
}
